{
    "name": "shengyou/travis-ci",
    "description": "a boilerplate project for PHP + Travis CI",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Shengyou Fan",
            "email": "shengyoufan@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "config": {
        "disable-tls": true,
        "secure-http": false
    }
}
